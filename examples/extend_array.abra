extend array<'a Equal> {
    fn find(self: array<'a Equal>, x: 'a Equal) -> maybe<int, void> {
        var i = 0
        while i < self.len() {
            if self[i] = x {
                return maybe.yes(i) 
            }
            i := i + 1
        }
        maybe.no(())
    }
}

let a = [11, 22, 33, 44, 55]
let index = a.find(33)
println("index is " & index)
let index = a.find(99999)
println("now index is " & index)

