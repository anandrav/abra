extend array<'a Equal> {
    fn find(self: array<'a Equal>, x: 'a Equal) -> int {
        var i = 0
        var ret = -1
        while i < self.len() {
            if self[i] = x {
                ret := i
            } else {
                //println("nah")
            }
            i := i + 1
        }
        return ret
    }
}

let a = [11, 22, 33, 44, 55]
let index = a.find(33)
println("index is " & index)
let index = a.find(99999)
println("now index is " & index)

