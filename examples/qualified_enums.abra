type Color = 
  | Red
  | Blue
  | Green
  | Custom of (int, int, int)

implement ToString for Color {
  fn str(c: Color) = match c {
    | .Red -> "red"
    | .Blue -> "blue"
    | .Green -> "green"
    // TODO: removing the qualifier below breaks it
    | Color.Custom(~r, ~g, ~b) -> "custom(" & r & "," & g & "," & b & ")"
  }
}

let c: Color = .Custom(1,2,3)
println(c)

