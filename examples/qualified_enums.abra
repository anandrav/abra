type Color = 
  | Red
  | Blue
  | Green
  | Custom of (int, int, int)

implement ToString for Color {
  fn str(c) = match c {
    | Color.Red -> "red" // the Color. prefix is unused right now
    | .Blue -> "blue"
    | .Green -> "green"
    | .Custom(~r, ~g, ~b) -> "custom(" & r & "," & g & "," & b & ")"
  }
}

let c = Color.Custom(1,2,3)
println(c)

